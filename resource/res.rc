    PUSHBUTTON      "OK", IDC_BTN_OK, 135, 5, 30, 10, 0, WS_EX_LEFT
}

LANGUAGE LANG_NEUTRAL, SUBLANG_NEUTRAL
IDD_ABOUT DIALOGEX 0, 0, 180, 80
STYLE DS_MODALFRAME | DS_SETFONT | WS_CAPTION | WS_VISIBLE | WS_POPUP | WS_SYSMENU
EXSTYLE WS_EX_TOOLWINDOW | WS_EX_STATICEDGE
CAPTION "About"
FONT 8, "MS Sans Serif", 0, 0, 1
{
    CTEXT           "http://warmode.net", IDC_ABOUT_URL, 30, 10, 150, 10, SS_LEFT, WS_EX_LEFT
    CTEXT           "wedis/0.0.1", IDC_ABOUT_NAME,      30, 20, 150, 10, SS_LEFT, WS_EX_LEFT
    CTEXT           "wedis is a simple yet powerful redis client.", IDC_ABOUT_DESCRIPTION,30, 30, 150, 10, SS_LEFT, WS_EX_LEFT
    CONTROL         "", IDC_ABOUT_IMAGE, WC_STATIC, WS_GROUP | SS_BITMAP, 7, 7, 20, 20, WS_EX_LEFT

    DEFPUSHBUTTON   "OK", IDC_ABOUT_OK, 67, 57, 32, 12, WS_GROUP, WS_EX_LEFT
    
}

