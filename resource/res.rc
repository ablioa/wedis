// 主菜单
ID_MAIN MENU DISCARDABLE 
BEGIN
    POPUP "文件(&F)"
    BEGIN
        MENUITEM "加载文件...",                 IDM_FILE_OPEN
        MENUITEM SEPARATOR
        MENUITEM "保存文件映像...",             IDM_FILE_DUMP
        MENUITEM "保存汇编文件...",             IDM_FILE_SAVESOURCE
        MENUITEM SEPARATOR
        MENUITEM "关闭",                        IDM_FILE_CLOSE
        MENUITEM SEPARATOR
        MENUITEM "退出",                        IDM_EXIT
    END
    POPUP "查看(&V)"
    BEGIN
        MENUITEM "Toolbar",                     IDM_VIEW_TOOLBAR
        MENUITEM SEPARATOR
        MENUITEM "Font...",                     IDM_VIEW_FONT
        MENUITEM "Color...",                    IDM_VIEW_COLOR
        MENUITEM SEPARATOR
        MENUITEM "Fullscreen",                  IDM_VIEW_FULLSCREEN
        MENUITEM "Saty Top",                    IDM_STAY
    END
    POPUP "模拟(&E)"
    BEGIN
        MENUITEM "重新启动",                    ID_MENUITEM4024
        MENUITEM "运行",                        ID_MENUITEM4028
        MENUITEM SEPARATOR
        MENUITEM "单步                  F7",    IDM_EXE_RUN
        MENUITEM SEPARATOR
        MENUITEM "转到...",                     IDM_VIEW_GOTOLINE
        MENUITEM "断点",                        IDM_EMU_SETBREAK
    END
    POPUP "选项(&O)"
    BEGIN
        MENUITEM "& 协议调试....",              IDM_PROTOCAL_DEBUG
    END
    POPUP "&帮助"
    BEGIN
        MENUITEM "&索引",                       IDM_HELP
        MENUITEM "&关于...",                    IDM_ABOUT
    END
END

// 调试菜单
IDM_DEBUG MENU DISCARDABLE 
BEGIN
    POPUP "网络(&N)"
    BEGIN
        MENUITEM "连接(&C)",                    IDM_CONNECT
        MENUITEM "断开[&D]",                    IDM_DISCONNECT
    END
END

