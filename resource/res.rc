{
    LISTBOX         IDC_LST_COLORS, 1, 1, 80, 178, WS_TABSTOP | WS_VSCROLL | NOT WS_BORDER | LBS_NOINTEGRALHEIGHT | LBS_NOTIFY, WS_EX_TRANSPARENT | WS_EX_STATICEDGE
    
    CONTROL         "", IDC_STATIC, WC_STATIC, SS_ETCHEDHORZ, 1, 180, 298, 1, WS_EX_LEFT
    PUSHBUTTON      "OK", IDOK,         108, 184, 32, 12, 0, WS_EX_STATICEDGE | BS_FLAT
    PUSHBUTTON      "Cancel", IDCANCEL, 180, 184, 32, 12, 0, WS_EX_STATICEDGE | BS_FLAT
}

LANGUAGE LANG_NEUTRAL, SUBLANG_NEUTRAL
IDD_PREFERENCE DIALOGEX 0, 0, 300, 200
STYLE DS_MODALFRAME | DS_SYSMODAL | DS_SETFONT | WS_CAPTION | WS_POPUP | WS_SYSMENU
EXSTYLE WS_EX_TOOLWINDOW | WS_EX_STATICEDGE
CAPTION "Preference"
FONT 8, "MS Sans Serif", 0, 0, 1
{
    CONTROL         "", IDC_STATIC, WC_STATIC, SS_ETCHEDHORZ, 1, 180, 298, 1, WS_EX_LEFT
    PUSHBUTTON      "OK",     IDC_CONFIG_DONE,  108, 184, 32, 12, 0, WS_EX_STATICEDGE
