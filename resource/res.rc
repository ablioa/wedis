// 跳转到指定行
IDD_GOTOLINE DIALOGEX 0, 0, 187, 68
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
EXSTYLE WS_EX_TOOLWINDOW | WS_EX_STATICEDGE
CAPTION "goto line"
FONT 8, "MS Sans Serif", 0, 0, 0x1
BEGIN
    LTEXT           "Please enter the address you want to jump\nto......",
                    IDC_STATIC,9,7,170,18
    EDITTEXT        IDC_ADDRESS,49,37,83,13,ES_AUTOHSCROLL | NOT WS_BORDER,
                    WS_EX_STATICEDGE
    LTEXT           "Address:",IDC_STATIC,11,39,36,11
