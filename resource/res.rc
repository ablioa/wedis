FONT 8, "MS Sans Serif", 0, 0, 1
{
    CONTROL         "", IDC_LIST_STATIC, WC_STATIC, SS_ETCHEDHORZ, 1, 100, 238, 1, WS_EX_LEFT
    PUSHBUTTON      "OK", IDC_LIST_OK,         38, 104, 32, 12, 0, WS_EX_STATICEDGE | BS_FLAT
    PUSHBUTTON      "Cancel", IDC_LIST_CANCEL, 100, 104, 32, 12, 0, WS_EX_STATICEDGE | BS_FLAT
}

LANGUAGE LANG_NEUTRAL, SUBLANG_NEUTRAL
IDD_CONNECTION DIALOGEX 0, 0, 300, 200
STYLE DS_MODALFRAME | DS_SYSMODAL | DS_SETFONT | WS_CAPTION | WS_POPUP | WS_SYSMENU
EXSTYLE WS_EX_TOOLWINDOW | WS_EX_STATICEDGE
CAPTION "Connection"
